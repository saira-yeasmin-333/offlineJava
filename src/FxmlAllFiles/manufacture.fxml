<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="682.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Client.Manufacture">
   <children>
      <TableView fx:id="carTableManufacturer" layoutY="-1.0" prefHeight="314.0" prefWidth="682.0">
        <columns>
          <TableColumn fx:id="regColumn" prefWidth="118.0" text="registrationNumber" />
          <TableColumn fx:id="colorColumn" onEditCommit="#editCarColour" prefWidth="119.0" text="Colour" />
            <TableColumn fx:id="carMakeColumn" onEditCommit="#editCarMake" prefWidth="101.0" text="carMake" />
            <TableColumn fx:id="carModelColumn" minWidth="3.0" onEditCommit="#editCarModel" prefWidth="96.0" text="carModel" />
            <TableColumn fx:id="yearColumn" minWidth="0.0" onEditCommit="#editYearMade" prefWidth="70.0" text="yearMade" />
            <TableColumn fx:id="priceColumn" onEditCommit="#editPrice" prefWidth="82.0" text="price" />
            <TableColumn fx:id="quantityColumn" onEditCommit="#editQuantity" prefWidth="92.0" text="quantity" />
        </columns>
      </TableView>
      <TextField fx:id="registrationField" layoutX="14.0" layoutY="324.0" prefHeight="25.0" prefWidth="86.0" promptText="Reg No" />
      <TextField fx:id="colorField" layoutX="113.0" layoutY="324.0" prefHeight="25.0" prefWidth="74.0" promptText="Colour" />
      <TextField fx:id="carModelField" layoutX="205.0" layoutY="324.0" prefHeight="25.0" prefWidth="86.0" promptText="Model" />
      <TextField fx:id="carMakeField" layoutX="306.0" layoutY="324.0" prefHeight="25.0" prefWidth="86.0" promptText="carMake" />
      <TextField fx:id="yearField" layoutX="400.0" layoutY="324.0" prefHeight="25.0" prefWidth="86.0" promptText="Year" />
      <TextField fx:id="priceField" layoutX="503.0" layoutY="324.0" prefHeight="25.0" prefWidth="74.0" promptText="Price" />
      <TextField fx:id="quantityField" layoutX="594.0" layoutY="324.0" prefHeight="25.0" prefWidth="74.0" promptText="Quantity" />
      <Button fx:id="add" layoutX="113.0" layoutY="361.0" mnemonicParsing="false" text="Add" />
      <Button fx:id="delete" layoutX="341.0" layoutY="361.0" mnemonicParsing="false" text="Delete" />
      <Button fx:id="logoutManufacturer" layoutX="514.0" layoutY="361.0" mnemonicParsing="false" text="Logout" />
   </children>
</AnchorPane>
